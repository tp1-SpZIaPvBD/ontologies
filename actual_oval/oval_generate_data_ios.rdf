    ns1:hasReference <https://tools.cisco.com/security/center/content/CiscoSecurityAdvisory/cisco-sa-20190327-cmp-dos> ;
    ns1:hasTitle "Cisco IOS and IOS XE Software Cluster Management Protocol Denial of Service Vulnerability" .

<oval:org.cisecurity:def:6258> a <https://oval.mitre.org/language/version5.11/OVAL> ;
