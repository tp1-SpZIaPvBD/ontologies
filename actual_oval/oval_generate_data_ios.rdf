    ns1:hasTitle "Cisco IOS Software Session Initiation Protocol Message Processing Arbitrary Code Execution Vulnerability" .

<oval:org.cisecurity:def:4332> a <https://oval.mitre.org/language/version5.11/OVAL> ;
    rdfs:label "Network Time Protocol Replay Attack on Authenticated Broadcast Mode Vulnerability" ;
