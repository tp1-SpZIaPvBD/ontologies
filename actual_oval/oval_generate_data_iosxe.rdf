<oval:org.cisecurity:def:7703> a <https://oval.mitre.org/language/version5.11/OVAL> ;
    rdfs:label "Cisco ASR 920 Series Aggregation Services Router Model 12SZ-IM SNMP Denial of Service Vulnerability (Medium Impact)" ;
    ns1:affectedFamily <http://www.semanticweb.org/rycht/ontologies/cyber_security_ontology/family#iosxe> ;
