    ns1:hasReference <https://tools.cisco.com/security/center/content/CiscoSecurityAdvisory/cisco-sa-20180328-privesc1> ;
    ns1:hasTitle "Cisco IOS XE Software User EXEC Mode Root Shell Access Vulnerability" .

